        {
            \large \sffamily \bfseries
            \fsr@institute \fsr@separator \fsr@subject
        }
        \hfill
        {
            \small \sffamily
            \fsr@semestr@text \fsr@semestr
        }
        \hfill
        {
            \small \sffamily \scshape
            \fsr@lector@text \fsr@lector
        }
        \nopagebreak \\
        {
            \small \ttfamily
            \fsr@texedby@text \fsr@texedby
        }
        % \nopagebreak \\[-4pt]
        % \vphantom{.}\hrulefill\vphantom{.} \\[4pt]
        \\[-4pt]\hhline\\[4pt]
        {
            \Large \bfseries
            \fsr@title
        }
        \hfill
        {
            \large \sffamily \itshape
            \fsr@date        
        }\par
    }
    \else
        \if\headertype\lection
        {
            \small\rmfamily
    		{\normalsize
    		    \centering{
    		        {
    		            \fsr@subject
    		        }
    		        \\
    			    \textsc
    			    {
    			        \fsr@lector@text \fsr@lector
    			    }
    		    }
    		    \\
    			{
    			    \fsr@texedby@text\fsr@texedby
    			}
    			\hfill\textit
    			{
    			    \fsr@institute \fsr@separator
    			    \fsr@semestr \fsr@semestr@text@var 
    			}  
    		}
    		% \nopagebreak \\[-4pt]
            % \vphantom{.}\hrulefill\vphantom{.} \\[4pt]
            \\[-4pt]\hhline\\[4pt]
            {
                \scshape
                \fsr@lection@text \fsr@lectnum
            }
            \hfill
            {
                \sffamily \itshape
                \fsr@date        
            }
            \\
            \centering{
                \large \scshape
                \fsr@title
            }\par
        }
        \else
        {
            \par
            \small\sffamily
			{
			    \normalsize\centering
			    {
			        \ttfamily \fsr@subject \rmfamily\\
				    \hfill\scshape \fsr@institute \upshape\hfill \hphantom{}\\[-0.7em]
			    }
			}
            \hhline
            % \vspace{-7px}
            {
                \large\sffamily
                \hphantom{}\hfill\textit{\fsr@date}\\
            }
            {
                \centering
                {
                    \Large
                    \textsc{\fsr@title}
                }
                \\[-1em]
            }
            % \strut \nopagebreak \rmfamily \normalsize
        }
        \fi
    \fi
}


\newcommand\header[1]
{   
    \if\headertype\lection
	{
    	\par \begingroup \normalsize
    	\strut
    	{
    	    \centering
    	    {
    	        \large 
    	        \textsc{#1}
    	    }
    	    \\[-1em]
    	}
    	\strut \nopagebreak
    	\endgroup
    }
    \else
    {
        \if\headertype\lection
        {
            \par \begingroup \normalsize
        	\strut
            {
                \Large \bfseries
                #1
            }
            \strut \nopagebreak
        	\endgroup
        }
        \else
        {
            \par
            {
                \centering
                {
                    \Large\textsc{#1}
                }
                \\[-1em]
            }
            \strut \nopagebreak
        }
        \fi
    }
    \fi
}

